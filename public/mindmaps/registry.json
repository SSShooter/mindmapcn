      "registryDependencies": [],
      "files": [
        {
          "path": "src/registry/mindmap.tsx",
          "type": "registry:ui",
          "target": "components/ui/mindmap.tsx"
        }
      ],
      "css": {
        "@layer base": {
          ".mindelixir-popup-content": {
             "@apply bg-transparent! shadow-none! p-0! rounded-none!": {}
          },
          ".mindelixir-popup-tip": {
            "@apply hidden!": {}
          }
        }
      }
    }
  ]
}
